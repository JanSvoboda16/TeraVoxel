{
  "Settings": {
    "Storage": {
      "StoragePath": "C:/storage", // Fill in path to your storage (can be empty folder)
      "SourceFileDirectory": "SourceFile",
      "DataDirectory": "Data",
      "SegmentSize": 256,
      "ProcessLayerSize": 8, // Min is 8
      "CompressionLevel": 0 // optional:0, fastest:1, noCompression:2, smallestSize:3
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
